{
  "name": "tantee-common",
  "version": "1.0.30",
  "description": "Common Vue/Vuetify library for TaNteE projects",
  "main": "dist/tantee-common.ssr.js",
  "browser": "dist/tantee-common.esm.js",
  "module": "dist/tantee-common.esm.js",
  "unpkg": "dist/tantee-common.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "lib/**"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "@zxing/library": "^0.18.4",
    "axios": "^0.21.1",
    "exif-rotate-js": "^1.3.0",
    "file-saver": "^2.0.5",
    "fuse.js": "^6.4.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "papaparse": "^5.3.0",
    "qs": "^6.9.6",
    "signature_pad": "^3.0.0-beta.4",
    "ttl-localstorage": "^1.5.4",
    "vue-async-computed": "^3.9.0",
    "vue-codemirror": "^4.0.6",
    "vue-cropperjs": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@mdi/font": "^5.9.55",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-service": "^4.5.13",
    "@vuedoc/md": "^3.2.0",
    "@vuedoc/parser": "^3.3.0",
    "cross-env": "^7.0.3",
    "material-design-icons-iconfont": "^6.1.0",
    "minimist": "^1.2.5",
    "rollup": "^2.36.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "rollup-plugin-vuetify": "^0.2.4",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "vue": "^2.6.12",
    "vue-cli-plugin-vuetify": "^2.2.2",
    "vue-template-compiler": "^2.6.12",
    "vuetify": "^2.4.4",
    "vuetify-loader": "^1.7.2",
    "vuex": "^3.6.2"
  },
  "peerDependencies": {
    "vue": "^2.6.12",
    "vuetify": "^2.4.4"
  },
  "engines": {
    "node": ">=12"
  }
}
